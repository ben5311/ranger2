Entity Kunde {
    id: 1
    name: random(["Max", "Peter", "John"])
    age: random(20..60),
    account: Account
}

Entity Account {
    id: 1
    balance: 15000
    currency: "EUR"
    cards: [1, 2, 3]
    createdAt: "2023-04-02"
    isActive: true
}

Entity References {
    first: 1
    second: first
    third: second
    currency: Kunde.account.currency
}
