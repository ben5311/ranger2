Entity Kunde {
    name: 1
    age: 2
    account: Account
    address: Address
    nex: random([10, 20, 30])
    nex2: random(1.0..2)
}

Entity Account {
    id: 1
    balance: 15000
    currency: "EUR"
    cards: [1, 2, 3]
    createdAt: "2023-04-02"
    isActive: true
}

Entity Address {
    address: "Hauptstr. 1"
    zip: "53111"
    city: "Bonn"
    country: "DE"
}

Entity References {
    first: 1
    second: first
    third: second
    currency: Kunde.account.currency
}
