{
    "name": "ranger",
    "displayName": "Ranger",
    "description": "A Test data generator",
    "version": "0.1.0",
    "publisher": "bheimann",
    "icon": "../../images/icon.png",
    "categories": ["Testing"],
    "repository": {
        "type": "git",
        "url": "https://github.com/ben5311/ranger2"
    },
    "engines": {
        "node": ">=14.0.0"
      },
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "files": [
        "src",
        "bin",
        "lib"
    ],
    "bin": {
        "ranger": "./bin/ranger"
    },
    "scripts": {
        "build": "langium generate",
        "watch": "langium generate --watch",
        "bundle": "esbuild ./src/cli/index.ts --bundle --outdir=out --format=cjs --platform=node --minify --target=es2021",
        "package": "npm run bundle && pkg ./out/index.js -o ranger -t win && pkg ./out/index.js -o ranger -t linux"
    },
    "dependencies": {
        "chalk": "~4.1.2",
        "chevrotain": "~10.4.2",
        "cli-progress": "~3.12.0",
        "commander": "~10.0.0",
        "csv": "~6.2.8",
        "langium": "~1.1.0",
        "lodash": "~4.17.21",
        "lru-cache": "^9.1.2",
        "luxon": "^3.3.0",
        "randexp": "~0.5.3",
        "random-js": "~2.1.0",
        "vscode-languageclient": "~8.0.2",
        "vscode-languageserver": "~8.0.2",
        "vscode-uri": "~3.0.7"
    },
    "devDependencies": {
        "@types/cli-progress": "~3.11.0",
        "@types/lodash": "~4.14.192",
        "@types/luxon": "^3.3.0",
        "@types/node": "~16.18.11",
        "@types/tmp": "~0.2.3",
        "@types/vscode": "~1.67.0",
        "@vscode/vsce": "~2.19.0",
        "dedent-js": "~1.0.1",
        "langium-cli": "~1.1.0",
        "pkg": "~5.8.1",
        "tmp": "~0.2.1"
    }
}
